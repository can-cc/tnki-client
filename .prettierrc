{
    "overrides": [
      {
        "files": ["**/*.vue"],
        "options": {
          "parser": "vue",
          "singleQuote": true,
          "printWidth": 100
        }
      },
      {
        "files": "**/*.scss",
        "options": {
          "parser": "css"
        }
      },
      {
        "files": "**/*.json",
        "options": {
          "parser": "json",
          "printWidth": 80
        }
      }
    ]
  }
  